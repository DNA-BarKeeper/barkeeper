<div class="panel panel-default">

  <div class="panel-heading">
    <h3 class="panel-title">Search Results for "<%= params[:search] %>"</h3>
  </div>

  <div class="panel-body">
    Hello, good user! <br>
    I found <%= pluralize(@result.size, 'record')%> for your search term "<%= params[:search] %>"!
  </div>
  <div>
    <%= multisearch_result(@result) %>
  </div>
</div>