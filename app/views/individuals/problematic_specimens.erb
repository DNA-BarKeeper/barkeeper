<!--problematic State/Province:-->
<div class="panel panel-default">

  <div class="panel-heading">
    <h3 class="panel-title">Specimens with problematic State/Province or Country entries:</h3>
  </div>

  <div class="panel-body">

    <div class="wideTable">

      <table class="table table-striped table-hover table-bordered">

        <thead>
        <tr>
          <th>Specimen ID</th>
          <th>State/Province</th>
          <th>Country</th>
        </tr>
        </thead>

        <tbody>

        <tr>
          <td></td>
          <td>Should be one of "Baden-Württemberg","Bayern","Berlin","Brandenburg","Bremen","Hamburg","Hessen","Mecklenburg-Vorpommern","Niedersachsen","Nordrhein-Westfalen","Rheinland-Pfalz","Saarland","Sachsen","Sachsen-Anhalt","Schleswig-Holstein","Thüringen"</td>
          <td>Should be "Deutschland"</td>
        </tr>

        <% @individuals.each do |individual| %>

            <tr>
              <td><%= link_to individual.specimen_id, edit_individual_path(individual) %></td>
              <td> <%= individual.state_province %></td>
              <td> <%= individual.country %></td>
            </tr>

        <% end %>
        </tbody>

      </table>
    </div>

    <br>

  </div>

</div>