
<div class="panel panel-default">
  
  <div class="panel-heading"><h3 class="panel-title"><%= @lab_rack.rackcode %></h3></div>
  
<div class="panel-body"><%= form_for(@lab_rack) do |f| %>
  
      <div class="form-group">
        <%= f.label :rackcode %>
        <%= f.text_field :rackcode, :autofocus => true, class: 'form-control' %>
      </div>

      <div class="form-group">
        <%= f.label 'Freezer' %>
        <%= f.collection_select(:freezer_id, Freezer.all, :id, :freezercode) %>
      </div>

      <%= f.submit 'Update', :class => 'btn btn-default' %>
<% end %></div>
</div>