<div class="panel panel-default">
  
  <div class="panel-heading"><h3  class="panel-title">Labs</h3></div>

  <div class="panel-body"><table class="table table-striped table-hover table-bordered">

  <thead>
    <tr>
      <th>Labcode</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @labs.each do |lab| %>
      <tr>
        <td><%= link_to lab.labcode, edit_lab_path(lab) %></td>
        <td><%= link_to 'Delete', lab, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table></div>

</div>

<p>
  <%= link_to new_lab_path, class: 'btn btn-default', title: 'Create a new lab entry' do %>
    <span class="glyphicon glyphicon-unchecked"></span>
  <% end %>
  Create a new lab entry
</p>
