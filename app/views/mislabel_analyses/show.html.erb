<div class="panel panel-default">
  <div class="panel-heading">
    <p><h3 class="panel-title">Sequences identified as mislabeled in SATIVA Analysis <%= title(@mislabel_analysis) %></h3></p>
    <p>
      <%= percentage_mislabels(@mislabel_analysis) %>
    </p>
  </div>
  <div class="panel-body">

    <div class="wideTable">
      <table id="mislabel_analysis_results" data-source="<%= mislabel_analysis_url(@mislabel_analysis, format: 'json') %>" class="table table-striped table-hover table-bordered" style="width: 100%;" >
        <thead>
        <tr>
          <th>Name</th>
          <th>Species</th>
          <th>Mislabel level</th>
          <th>Confidence</th>
          <th>Proposed Label</th>
          <th>Last updated</th>
          <th></th>
        </tr>
        </thead>

        <tbody>
        </tbody>
      </table>
    </div>

  </div>
</div>

<p>
  <%= link_to 'Go back to all SATIVA analyses', mislabel_analyses_path, class: 'btn btn-default' %>
</p>
