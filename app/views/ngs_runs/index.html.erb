<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Next Generation Sequencing Runs</h3>
  </div>
  <div class="panel-body">
    <div class="wideTable">
      <table id="ngs_runs" data-source="<%= ngs_runs_path(format: "json") %>" class="table table-striped table-hover table-bordered" >
        <thead>
        <tr>
          <th>Name</th>
          <th>Last updated</th>
          <th></th>
        </tr>
        </thead>
        <tbody>
        </tbody>
      </table>
    </div>
  </div>
</div>

<% if user_signed_in? %>
  <%= link_to 'New NGS Run', new_ngs_run_path, class: 'btn btn-default' %>
<% end %>

