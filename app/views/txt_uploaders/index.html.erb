<p id="notice"><%= notice %></p>

<h1>Listing Txt Uploaders</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @txt_uploaders.each do |txt_uploader| %>
      <tr>
        <td><%= link_to 'Show', txt_uploader %></td>
        <td><%= link_to 'Edit', edit_txt_uploader_path(txt_uploader) %></td>
        <td><%= link_to 'Destroy', txt_uploader, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Txt uploader', new_txt_uploader_path %>
